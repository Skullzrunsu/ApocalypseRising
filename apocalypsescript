if not folder then
	local s = Instance.new('Folder')
	s.Name = "ESPFolder"
	s.Parent = LocalPlayer.PlayerGui
	
	folder = s
elseif folder then
	folder:ClearAllChildren()
end


local function client_message(text, isGood)
	LocalPlayer.PlayerGui.MessageSystem.NewMessage.Color.Value = isGood and 'Green' or 'Red'
	LocalPlayer.PlayerGui.MessageSystem.NewMessage.Value = text
end

