ARApocGUI.Name = "AR Apoc GUI"
ARApocGUI.Parent = scripts
ARApocGUI.BackgroundColor3 = Color3.new(0.117647, 0.117647, 0.117647)
ARApocGUI.BorderSizePixel = 0
ARApocGUI.Position = UDim2.new(0, 0, 0, 243)
ARApocGUI.Size = UDim2.new(0, 123, 0, 40)
ARApocGUI.Font = Enum.Font.SourceSansBold
ARApocGUI.Text = "AR Apoc GUI"
ARApocGUI.TextColor3 = Color3.new(0.666667, 0, 0)
ARApocGUI.TextSize = 16
ARApocGUI.MouseButton1Down:connect(function()
	--RR Apoc Gui V2.3 Made by RelentlessRaptor/!!!RelentlessRaptor on discord
