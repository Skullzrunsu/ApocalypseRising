function getMatches(L_85_arg1)
	local L_86_ = {}
	for L_87_forvar1 = 1, #L_56_ do
		local L_88_ = L_56_[L_87_forvar1].Name:lower()
		if L_88_:find(L_85_arg1:lower()) then
			table.insert(L_86_, L_56_[L_87_forvar1])
		end
	end;
	table.sort(L_86_, alphabetize)
	return L_86_
end;
