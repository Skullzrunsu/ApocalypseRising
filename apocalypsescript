local function RetrieveObject(Name)
	return AR_UI.Background:FindFirstChild(Name, true);
end;

local function ChangeParent(...)
	Remotes.GrabItem:FireServer(...);
end;

local fillers = {"a","B","i","P","Y","S"}
local alphabet = {"A","C","d","g","L","p","s","T","Z"}

function FindMultiplier(part)
	for z = 1, #alphabet do
		if alphabet[z] == part then
			return z
		end
	end
	
	return "0" 
end

local function Obfuscate(val)
   local newval = ""
   val = tostring(val)
   
   for _ = 1, 10 - string.len(val) do
       newval = fillers[math.random(1,6)] .. newval
   end
   
   for d = 1, string.len(val) do
       local num = tonumber(string.sub(val, d, d))
       if num > 0 then
           newval = newval .. alphabet[num]
       else
           newval = newval .. fillers[math.random(1,6)]
       end
   end
   
   return newval 
end

local function Deobfuscate(val)
	local newval = ""
	
	for d = 1, 10 do
		local mult = FindMultiplier(string.sub(val,d,d))
		newval = newval .. mult
	end
	
	newval = tonumber(newval)
	return newval 
end

local function get_weapons(p)
	if p then
		local pack = p.Backpack
		local guns = {}
		
		for k, v in next, pack:GetChildren() do
			if Weapons:FindFirstChild(v.Name) then
				table.insert(guns, v)
			end
		end
		
		return guns
	end
end 

local function SpawnItem(Item, Amount)
	pcall(function()
		if not Amount then
			Amount = 1
		end
		
		local Amount = Amount < 1 and 1 or Amount > 15 and 15 or Amount
		warn(Amount, 'oof')
		for Key = 1, Amount do
			local SpawnedItem = loot_drops:FindFirstChild(Item)
			
			if SpawnedItem then
				Remotes.PlaceMaterial:FireServer(SpawnedItem, LocalPlayer.Character.Torso.Position - SpawnedItem.PrimaryPart.Position + Vector3.new(math.random(1, 10), 0, math.random(1, 10)))
			end
		end;
	end);
end;

local function GetPlayer(String)
	if String == 'me' then
		return LocalPlayer
	end
	
	if game:GetService("Players"):FindFirstChild(String) then
		return game:GetService("Players"):FindFirstChild(String)
	end
	
	for k, v in next, game:GetService('Players'):GetPlayers() do
		if string.sub(v.Name, 1, string.len(String)):lower() == String then
			return v
		end
	end
	
	return nil
end


do
	for k, v in next, loot_drops:GetChildren() do
		local loot = Instance.new('TextButton')
		loot.Name = v.Name
		loot.Text = v.Name
		loot.Position  = UDim2.new(0, 0, 0, (25 * k) - 25)
		loot.Size = UDim2.new(0, 125, 0, 25)
		loot.BackgroundColor3 = Color3.fromRGB(121, 121, 121)
		loot.BorderSizePixel = 0
		loot.TextColor3 = Color3.fromRGB(255, 255, 255)
		loot.Font = Enum.Font.SourceSansBold
		loot.TextSize = 14
		
		loot.Parent = RetrieveObject("LootList")
		loot.MouseButton1Click:connect(function()
			RetrieveObject('Input').Text = loot.Text
		end)
	end
	
	RetrieveObject('LootList').CanvasSize = UDim2.new(0, 0, 0, (#loot_drops:GetChildren() * 25))
end

RetrieveObject('Input').FocusLost:connect(function(enter)
	if enter then
		if RetrieveObject('Input').Text:gsub(" ", "") == "" then
			return
		end
		
		if GetPlayer(RetrieveObject('Input').Text) then
			RetrieveObject('Input').Text = GetPlayer(RetrieveObject('Input').Text).Name
		end
	end
end)

local items = {'Patriot', 'Katana'}
for k, v in next, items do
	if game:GetService('Lighting').LootDrops:FindFirstChild(v) then
		local thingo = game:GetService('Lighting').LootDrops:FindFirstChild(v)
		thingo.PrimaryPart = thingo:FindFirstChild('Model').Head
	end
end

local Colors = {
	"White";
	"Red";
	"Blue";
	"Yellow";
	"Green";
};

local CurrentColor = 1;

RetrieveObject("Godmode").MouseButton1Click:Connect(function()
	pcall(function()
		Remotes.AddDamageSelf:FireServer(LocalPlayer.Character.Humanoid, math.huge)
		Settings.GodMode = true
		client_message("Godmode is enabled.", true)
	end)
end);

RetrieveObject("Kick").MouseButton1Click:Connect(function()
	pcall(function()
		ChangeParent(nil, nil, GetPlayer(RetrieveObject("Input").Text));
	end);
end);

RetrieveObject("Kill").MouseButton1Click:Connect(function()
	pcall(function()
		ChangeParent(nil, nil, GetPlayer(RetrieveObject("Input").Text).Character.Torso.Neck);
	end);
end);

RetrieveObject("NoRecoil").MouseButton1Click:Connect(function()
	pcall(function()
		
		local player = GetPlayer(RetrieveObject('Input').Text)
		
		if player then
			if Settings.PlayerToggles.Recoil[player] then
				Settings.PlayerToggles.Recoil[player] = not Settings.PlayerToggles.Recoil[player] 
			else
				Settings.PlayerToggles.Recoil[player] = true
			end
			
			local toggle = Settings.PlayerToggles.Recoil[player]
			
			if toggle == true then -- recoil (on)
				local things = get_weapons(player)
				
				for k, v in next, things do
					warn(v)
					local stat = v:FindFirstChild('Stats').Recoil
					local recoil = Obfuscate(1)
					Remotes.GroupLeader:FireServer({Name = recoil}, {member1 = stat}, nil)
				end
			elseif toggle == false then -- recoil off			
				local things = get_weapons(player)
						
				for k, v in next, things do
					local stat = v:FindFirstChild('Stats').Recoil
					local new_recoil = Weapons:FindFirstChild(v.Name).Stats.Recoil.Value
						
					Remotes.GroupLeader:FireServer({Name = new_recoil}, {member1 = stat}, nil)
				end
			end
			
			client_message("No recoil for " .. player.Name .. (toggle and " is enabled!" or " is disabled!"), toggle)
		end
	end)
end);

RetrieveObject("ServerMessage").MouseButton1Click:Connect(function()
	pcall(function()
		for i, player in next, game:GetService("Players"):GetPlayers() do
			local flag = RetrieveObject("Value").Text == "Red" or RetrieveObject("Value").Text == "Blue" or RetrieveObject("Value").Text == "Green" or RetrieveObject("Value").Text == "White" or RetrieveObject("Value").Text == "Yellow" 
			if flag then
				color = RetrieveObject("Value").Text 
			else
				color = Colors[CurrentColor]
			end
			Remotes.SendMessage:FireServer(player, color, RetrieveObject("Input").Text)
		end
		
		CurrentColor = (CurrentColor + 1);
		if (CurrentColor > #Colors) then
			CurrentColor = 1;
		end;
	end);
end);

RetrieveObject("Goto").MouseButton1Click:connect(function()
	pcall(function()
		local player = GetPlayer(RetrieveObject("Input").Text)
		
		if player then
			local character = player.Character
			Remotes.AddDamageSelf:FireServer(LocalPlayer.Character.Humanoid, math.huge)
			Settings.GodMode = true
			LocalPlayer.Character.HumanoidRootPart.CFrame = character.HumanoidRootPart.CFrame + Vector3.new(0, 5, 0)
			
			client_message('Teleported to: ' .. player.Name, true)	
		end
	end)
end)

local ChamsPointers = {};
RetrieveObject("Bring").MouseButton1Click:connect(function()
	pcall(function()
		Settings.ESP = not Settings.ESP
		local toggle = Settings.ESP
		
		if toggle == true then
			for i, player in next, game:GetService("Players"):GetPlayers() do
				local p_folder = Instance.new('Folder', folder)
				p_folder.Name = player.Name
				local character = player.Character
				
				if player ~= LocalPlayer then
					for k, v in next, character:GetChildren() do
						if v:IsA('BasePart') then
							for i, face in next, Enum.NormalId:GetEnumItems() do
								local surface = Instance.new("SurfaceGui", p_folder)
								local base = Instance.new('Frame', surface)
								
								surface.AlwaysOnTop = true
								surface.Adornee = v
								surface.Face = face
								surface.Name = face.Name
									
								base.Size = UDim2.new(1, 0, 1, 0)
								base.BorderSizePixel = 0
								base.BackgroundColor3 = Color3.new(0, 1, 0)
								base.BackgroundTransparency = 0.5
							end
						end
					end
				end
			end
		else
			folder:ClearAllChildren()
		end
		
		client_message("ESP is" .. (toggle and " enabled!" or " disabled!"), toggle)
	end)
end)

RetrieveObject("Spam").MouseButton1Click:Connect(function()
	Settings.SpamChat = (not Settings.SpamChat);
	RetrieveObject('Spam').Text = 'Chat Spam: ' .. (Settings.SpamChat == true and "On" or "Off")
end);

RetrieveObject("Spawn").MouseButton1Click:Connect(function()
	pcall(function()
		SpawnItem(RetrieveObject("Input").Text, tonumber(RetrieveObject("Value").Text));
	end);
end);

RetrieveObject("SuperJump").MouseButton1Click:Connect(function()
	pcall(function()
		local Humanoid = LocalPlayer.Character.Humanoid;
		RetrieveObject("SuperJump").Text = "Super Jump: " .. (Humanoid["JumpPower"] == 125 and "Off" or "On")
		Humanoid.JumpPower = (Humanoid["JumpPower"] == 125 and 50 or 125)
	end);
end);

RetrieveObject("Corpses").MouseButton1Click:Connect(function()
	pcall(function()
		for Key, Value in next, workspace:GetChildren() do
			if (Value.Name == "Corpse") then
				Value:MoveTo(
					workspace[LocalPlayer.Name].Torso.Position +
					Vector3.new(math.random(-15, 15), 0, math.random(-15, 15))
				)
			end;
		end;
	end);
end);

game:GetService("UserInputService").InputBegan:Connect(function(input, gpe)
	pcall(function()
		if not gpe and input.UserInputType == Enum.UserInputType.Keyboard then
			if input.KeyCode == Enum.KeyCode.L then
				AR_UI.Background.Visible = not AR_UI.Background.Visible
			end
		end
	end)
end);

RetrieveObject('InfiniteAmmo').MouseButton1Click:connect(function()
	pcall(function()
		local player = GetPlayer(RetrieveObject('Input').Text)
		
		if player then
			local stats = player:FindFirstChild('playerstats')
			
			if Settings.PlayerToggles.InfiniteAmmo[player] then
				Settings.PlayerToggles.InfiniteAmmo[player] = not Settings.PlayerToggles.InfiniteAmmo[player] 
			else
				Settings.PlayerToggles.InfiniteAmmo[player] = true
			end
			
			local toggle = Settings.PlayerToggles.InfiniteAmmo[player]
			
			if toggle == true then -- turn infinite ammo on
				for k, v in next, stats.slots:GetChildren() do
					if v:FindFirstChild('ObjectID') and v:FindFirstChild('ObjectID'):FindFirstChild('Clip') then
						local clip =  v:FindFirstChild('ObjectID'):FindFirstChild('Clip')
						
						Remotes.GroupLeader:FireServer({Name = 1000000000}, {member1 = clip.MaxClip}, nil)
						Remotes.GroupLeader:FireServer({Name = Obfuscate(1000000000)}, {member1 = clip}, nil)
					end
				end
			elseif toggle == false then -- revert the ammo
				for k, v in next, stats.slots:GetChildren() do
					if v:FindFirstChild('ObjectID') and v:FindFirstChild('ObjectID'):FindFirstChild('Clip') then
						for i, object in next, loot_drops:GetChildren() do
							if object:FindFirstChild('ObjectID') and object:FindFirstChild('ObjectID'):FindFirstChild('Clip') then
								if Deobfuscate(object:FindFirstChild('ObjectID').Value) == Deobfuscate(v:FindFirstChild('ObjectID').Value) then
									local clip =  v:FindFirstChild('ObjectID'):FindFirstChild('Clip')
									local new_clip = object:FindFirstChild('ObjectID'):FindFirstChild('Clip')
									
									Remotes.GroupLeader:FireServer({Name = new_clip.Value}, {member1 = clip}, nil)
									Remotes.GroupLeader:FireServer({Name = new_clip.MaxClip.Value}, {member1 = clip.MaxClip}, nil)
								end
							end
						end
					end
				end
			end
			
			client_message("Infinite Ammo for " .. player.Name .. (toggle and " is enabled!" or " is disabled!"), toggle)
		end
	end)
end)

pcall(function()
	ChangeParent(nil, nil, Remote.GetSkins)
end)

game:GetService("Players").PlayerAdded:connect(function(player)
	wait(1)
	if player:FindFirstChild("privilege") then
		ChangeParent(nil, nil, player)
		client_message("[AUTO] Kicked moderator: " .. player.Name, true)
	end
	
	player.CharacterAdded:connect(function()
		if Settings.ESP then
			local xd = folder:FindFirstChild(player.Name)
			
			if not xd then
				xd = Instance.new('Folder', folder)
				xd.Name = player.Name
			end
				
			for k, v in next, character:GetChildren() do
				if v:IsA('BasePart') then
					for i, face in next, Enum.NormalId:GetEnumItems() do
						local surface = Instance.new("SurfaceGui", xd)
						local base = Instance.new('Frame', surface)
							
						surface.AlwaysOnTop = true
						surface.Adornee = v
						surface.Face = face
						surface.Name = face.Name
							
						base.Size = UDim2.new(1, 0, 1, 0)
						base.BorderSizePixel = 0
						base.BackgroundColor3 = Color3.new(0, 1, 0)
						base.BackgroundTransparency = 0.5
					end
				end
			end
		end
	end)
end)

game:GetService("Players").PlayerRemoving:connect(function(player)
	if folder:FindFirstChild(player.Name) then
		folder[player.Name]:Destroy()
	end
end)

for i, player in next, game:GetService("Players"):GetPlayers() do
	if player:FindFirstChild("privilege") then
		ChangeParent(nil, nil, player)
		client_message("[AUTO] Kicked moderator: " .. player.Name, true)
	end
	
	player.CharacterAdded:connect(function()
		if Settings.ESP then
			local xd = folder:FindFirstChild(player.Name)
			
			if not xd then
				xd = Instance.new('Folder', folder)
				xd.Name = player.Name
			end
			
			local character = player.Character
				
			for k, v in next, character:GetChildren() do
				if v:IsA('BasePart') then
					for i, face in next, Enum.NormalId:GetEnumItems() do
						local surface = Instance.new("SurfaceGui", xd)
						local base = Instance.new('Frame', surface)
							
						surface.AlwaysOnTop = true
						surface.Adornee = v
						surface.Face = face
						surface.Name = face.Name
							
						base.Size = UDim2.new(1, 0, 1, 0)
						base.BorderSizePixel = 0
						base.BackgroundColor3 = Color3.new(0, 1, 0)
						base.BackgroundTransparency = 0.5
					end
				end
			end
		end
	end)
end

spawn(function()
	local Spams = Settings.Spams;
	while true do
		if Settings.SpamChat then
			pcall(function()
				Remotes.Chat:FireServer("Global", Spams[math.random(1, #Spams)]);
			end);
		end;
		
		if Settings.GodMode then
			LocalPlayer.Character.Humanoid.MaxHealth = math.huge
			LocalPlayer.Character.Humanoid.Health = math.huge
		end
		
		wait(0.1);
	end;
end);

client_message("AR Tools 3.0 loaded [" .. tostring(tick() - start):sub(1, 5) .. " seconds]", true)
client_message("Made by MedievalBeast4, credits to wallythemad for Infinite Ammo, No Recoil, Godmode and TP.", true)
end)

